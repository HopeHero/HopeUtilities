{
  "name": "Hope Utilities",
  "description": "hope`s Utilitie program for Chrome and Microsoft!",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "/assets/scripts/background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "logo": "/assets/images/image.png"
    }
  },
  "icons": {
    "logo": "/assets/images/image.png"
  }
}
