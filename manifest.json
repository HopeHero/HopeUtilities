{
  "name": "Hope Utilities",
  "description": "hope`s Utilitie program for Chrome and Microsoft!",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "/assets/scripts/background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/images/logo16.png",
      "32": "/assets/images/logo32.png",
      "48": "/assets/images/logo48.png",
      "128": "/assets/images/logo128.png"
    }
  },
  "icons": {
    "16": "/assets/images/logo16.png",
    "32": "/assets/images/logo32.png",
    "48": "/assets/images/logo48.png",
    "128": "/assets/images/logo128.png"
  }
}
